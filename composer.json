{
    "name": "Design-Collective/magento-bootstrap-sass",
    "type": "magento-module",
    "description": "HTML5 Twitter Bootstrap 3.1 Magento Boilerplate Template",
    "authors": [
        {
            "name": "Design Collective",
            "email": "bencorlett@me.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/magento-hackathon/magento-composer-installer"
        }
    ],
    "require": {
        "magento-hackathon/magento-composer-installer":"*"
    },
    "extra": {
        "map": [
            ["app/code/local/Bootstrap/BootstrapNavigation", "app/code/local/Bootstrap/BootstrapNavigation"],
            ["app/design/frontend/bootstrap-sass/default", "app/design/frontend/bootstrap-sass/default"],
            ["app/etc/modules/BootstrapNavigation.xml", "app/etc/modules/BootstrapNavigation.xml"],
            ["skin/frontend/bootstrap-sass/default", "skin/frontend/bootstrap-sass/default"]
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}